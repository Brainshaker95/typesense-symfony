{
  "$schema": "https://getcomposer.org/schema.json",
  "name": "brainshaker95/typesense-symfony",
  "version": "1.0.0",
  "description": "Demo Typesense implementation in a Symfony project",
  "license": "MIT",
  "authors": [
    {
      "name": "Patrick Rupp"
    }
  ],
  "homepage": "https://github.com/Brainshaker95/typesense-symfony",
  "support": {
    "issues": "https://github.com/Brainshaker95/typesense-symfony/issues",
    "source": "https://github.com/Brainshaker95/typesense-symfony"
  },
  "type": "project",
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "require": {
    "php": ">=8.4",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "phpdocumentor/reflection-docblock": "^5.6.3",
    "phpstan/phpdoc-parser": "^2.3",
    "symfony/console": "7.3.*",
    "symfony/dotenv": "7.3.*",
    "symfony/flex": "^2.8.2",
    "symfony/form": "7.3.*",
    "symfony/framework-bundle": "7.3.*",
    "symfony/http-client": "^7.3.4",
    "symfony/monolog-bundle": "^3.10",
    "symfony/property-access": "7.3.*",
    "symfony/property-info": "7.3.*",
    "symfony/runtime": "7.3.*",
    "symfony/serializer": "7.3.*",
    "symfony/string": "7.3.*",
    "symfony/twig-bundle": "7.3.*",
    "symfony/validator": "7.3.*",
    "symfony/yaml": "7.3.*",
    "twig/twig": "^3.21",
    "typesense/typesense-php": "^5.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "3.75.0",
    "kubawerlos/php-cs-fixer-custom-fixers": "3.27.0",
    "phpstan/extension-installer": "1.4.3",
    "phpstan/phpstan": "2.1.17",
    "phpstan/phpstan-deprecation-rules": "2.0.3",
    "phpstan/phpstan-doctrine": "2.0.3",
    "phpstan/phpstan-phpunit": "2.0.6",
    "phpstan/phpstan-strict-rules": "2.0.4",
    "phpstan/phpstan-symfony": "2.0.6",
    "phpstan/phpstan-webmozart-assert": "2.0.0",
    "rector/rector": "2.0.8",
    "rector/type-perfect": "2.1.0",
    "symplify/phpstan-rules": "14.6.9",
    "ticketswap/phpstan-error-formatter": "1.1.5"
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "extra": {
    "symfony": {
      "require": "7.3.*"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "bump-after-update": true,
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true,
      "phpstan/extension-installer": true,
      "symfony/flex": true,
      "symfony/runtime": true
    }
  }
}
